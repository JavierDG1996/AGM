<AGMModel>
	<!-- Human -->
	<symbol id="human" type="human" />
	<!-- Coffee -->
	<symbol  id="coffeeObjectHuman"  type="object" />
	<symbol  id="coffeeReachHuman"   type="reach" />
	<symbol  id="coffeeTypeHuman"    type="coffeePot" />
	<link   src="coffeeObjectHuman"  dst="coffeeReachHuman"  label="prop" />
	<link   src="coffeeObjectHuman"  dst="coffeeTypeHuman"   label="prop" />
	<!-- Mug -->
	<symbol  id="mugObjectHuman"     type="object" />
	<symbol  id="mugReachHuman"      type="reach" />
	<symbol  id="mugTypeHuman"       type="mug" />
	<link   src="mugObjectHuman"     dst="mugReachHuman"     label="prop" />
	<link   src="mugObjectHuman"     dst="mugTypeHuman"      label="prop" />
	<!-- Milk -->
	<symbol  id="milkObjectHuman"    type="object" />
	<symbol  id="milkReachHuman"     type="reach" />
	<symbol  id="milkTypeHuman"      type="milkPot" />
	<link   src="milkObjectHuman"    dst="milkReachHuman"    label="prop" />
	<link   src="milkObjectHuman"    dst="milkTypeHuman"     label="prop" />
	<!-- Human and the objects he knows -->
	<link src="human" dst="coffeeObjectHuman" label="know" />
	<link src="human" dst="mugObjectHuman"    label="know" />
	<link src="human" dst="milkObjectHuman"   label="know" />


	<!-- Robot -->
	<symbol id="robot" type="robot" />
	<!-- Coffee -->
	<symbol  id="coffeeObjectRobot"  type="object" />
	<symbol  id="coffeeReachRobot"   type="reach" />
	<symbol  id="coffeeTypeRobot"    type="coffeePot" />
	<link   src="coffeeObjectRobot"  dst="coffeeReachRobot"  label="prop" />
	<link   src="coffeeObjectRobot"  dst="coffeeTypeRobot"   label="prop" />
	<!-- Mug -->
	<symbol  id="mugObjectRobot"     type="object" />
	<symbol  id="mugReachRobot"      type="reach" />
	<symbol  id="mugTypeRobot"       type="mug" />
	<link   src="mugObjectRobot"     dst="mugReachRobot"     label="prop" />
	<link   src="mugObjectRobot"     dst="mugTypeRobot"      label="prop" />
	<!-- Milk -->
	<symbol  id="milkObjectRobot"    type="object" />
	<symbol  id="milkReachRobot"     type="reach" />
	<symbol  id="milkTypeRobot"      type="milkPot" />
	<link   src="milkObjectRobot"    dst="milkReachRobot"    label="prop" />
	<link   src="milkObjectRobot"    dst="milkTypeRobot"     label="prop" />


	<!-- Human and the objects he knows -->
	<link src="robot" dst="coffeeObjectRobot" label="know" />
	<link src="robot" dst="mugObjectRobot"    label="know" />
	<link src="robot" dst="milkObjectRobot"   label="know" />

</AGMModel>

