LOADTRUCK : active(3)
{
	{
	}
	=>
	{
	}
	parameters
	{
		loc:LCTN
		obj:OBJ
		truck:TRCK
	}
	precondition
	{
		(and
			(at truck loc)
			(at obj loc)
		)
	}
	effect
	{
		(and
			(not (at obj loc))
			(at  obj truck)
		)
	}
}

