DRIVETRUCK : active(3)
{
	{
	}
	=>
	{
	}
	parameters
	{
		lo2:LCTN ci:CITY lo1:LCTN tr:TRCK
	}
	precondition
	{
		(and
			(at tr lo1)
			(inCity lo1 ci)
			(inCity lo2 ci)
		)
	}
	effect
	{
		(and
			(not (at tr lo1))
			(at tr lo2)
		)
	}
}

TRANSPORTinTRUCK : active(1)
{
	LOADTRUCK as load
	DRIVETRUCK as drive
	UNLOADTRUCK as unload
	where:
	load.truck = drive.truck = unload.truck
	load.loc = drive.lo1
	unload.loc = drive.lo2
	load.obj = unload.obj
}

