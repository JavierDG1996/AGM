	<symbol id="estado" type="objectSt" />		<!-- T A B L E   S T A T U S -->
	<symbol id="habitacion" type="object" /> 	<!-- R O O M -->

	<link  src="robot"	dst="habitacion"	label="in" />	<!-- Robot en habitacion-->
